{
  "expo": {
    "name": "VOUCH",
    "slug": "vouch-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.vouch.mobile",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "io.vouch.mobile"
    },
    "plugins": [
      "expo-dev-client"
    ]
  }
}
